<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>abcd</RootNamespace>
    <Authors>Murali Krishna KUNAPAREDDY</Authors>
    <Company>aBCDe</Company>
    <PackageId>abcd.ColoredConsole</PackageId>
    <Product>aBCDe's ColoredConsole</Product>
    <Description>A revised aBCDe's ColoredConsole helpful to build c# console applications with colored text and boxes</Description>
    <Copyright>2018, 2019,2020</Copyright>
    <Version>0.0.1</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>Revised initial release</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="ReadMe.txt">
      <Pack>true</Pack>
      <PackagePath>\</PackagePath>
    </Content>
  </ItemGroup>

</Project>
